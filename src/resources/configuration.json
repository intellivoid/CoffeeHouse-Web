{
    "primary_language": "en",
    "debugging_mode": false,
    "enable_ppm": true,
    "router": [
        {"path": "/", "page": "index"},
        {"path": "/auth/logout", "page": "logout"},
        {"path": "/service_error", "page": "service_error"},
        {"path": "/terms", "page": "tos"},
        {"path": "/privacy", "page": "privacy"},
        {"path": "/demo/lydia", "page": "lydia_demo"},
        {"path": "/confirm_purchase", "page": "purchase"},
        {"path": "/dashboard", "page": "dashboard"}
    ],
    "libraries": {
        "SecuredWebSessions": {
            "directory_name": "sws",
            "autoloader": "sws.php",
            "check_class_exists": true,
            "namespace": "sws",
            "main_class": "sws"
        },
        "IntellivoidSubscriptionManager": {
            "package_name": "net.intellivoid.subscriptions",
            "version": "latest",
            "import_dependencies": true,
            "throw_error": true
        },
        "IntellivoidAPI": {
            "package_name": "net.intellivoid.api",
            "version": "latest",
            "import_dependencies": true,
            "throw_error": true
        },
        "CoffeeHouse": {
            "package_name": "net.intellivoid.coffeehouse",
            "version": "latest",
            "import_dependencies": true,
            "throw_error": true
        },
        "COASniffle": {
            "package_name": "net.intellivoid.coa_sniffle",
            "version": "latest",
            "import_dependencies": true,
            "throw_error": true
        }
    },
    "runtime_scripts": {
        "initialize": ["coa_sniffles.php"],
        "on_page_load": ["check_auth.php"],
        "page_loaded": [],
        "after_request": [],
        "on_request": ["maintenance.php"],
        "on_import": []
    }
}